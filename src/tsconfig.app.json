{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "paths": {
      "@mn-core/*": [
        "mn-core/*"
      ],
      "@blocks/*": [
        "blocks/*"
      ],
      "@app-shared/*": [
        "app/shared/*"
      ],
      "@app-core/*": [
        "app/core/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}