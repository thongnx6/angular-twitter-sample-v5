{
  "name": "prj",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev:config": "ts-node ./src/set-env.ts  --environment=dev && ts-node ./src/set-proxy.ts  --environment=dev",
    "build:config": "ts-node ./src/set-env.ts  --environment=prod && ts-node ./src/set-proxy.ts  --environment=prod",
    "start": "npm run dev:config && ng serve --aot --environment=dev --proxy-config proxy.conf.json",
    "build": "npm run build:config && ng build --environment=prod --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean:coverage": "rimraf coverage",
    "test:coverage": "npm run clean:coverage && ng test --watch=false --code-coverage",
    "test:ci": "ng test --watch=false",
    "format:fix": "pretty-quick",
    "precommit": "lint-staged",
    "prepush": "ng test --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "bootstrap": "^4.1.1",
    "core-js": "^2.4.1",
    "dotenv": "^5.0.1",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^2.0.5",
    "rxjs": "^5.5.6",
    "yargs": "^11.0.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "argv": "0.0.2",
    "codelyzer": "^4.0.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.1.2",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.5.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}